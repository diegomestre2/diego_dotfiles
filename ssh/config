Host *
 AddKeysToAgent yes
 IgnoreUnknown UseKeychain
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa

Host doorgang
 HostName ssh.cwi.nl
 User tome
 IdentityFile ~/.ssh/id_cwi
 ForwardAgent=yes

Host cwi
 HostName chagres.da.cwi.nl
 User tome
 ProxyCommand ssh -W %h:%p doorgang
 IdentityFile ~/.ssh/id_rsa
 PasswordAuthentication yes

Host scilens
 HostName scilens2-ssh.da.cwi.nl
 User tome
 ProxyCommand ssh -W %h:%p doorgang
 IdentityFile ~/.ssh/id_rsa_scilens
 ForwardAgent=yes

Host president
 ForwardAgent=yes
 HostName thepresident.da.cwi.nl
 User tome
 Proxycommand ssh scilens nc %h 22
 IdentityFile ~/.ssh/id_cwi
 IdentityFile ~/.ssh/id_rsa_scilens

Host diamonds* stones* pebbles* rocks* bricks* emeralds* gems*
 ForwardAgent=yes
 ProxyCommand ssh scilens nc %h 22
 User tome
 IdentityFile ~/.ssh/id_cwi
 IdentityFile ~/.ssh/id_rsa_scilens
 StrictHostKeyChecking no
 UserKnownHostsFile /dev/null
 LogLevel Error

Host lab*
 HostName %h.da.cwi.nl
 User tome
 ProxyCommand ssh -W %h:%p doorgang
 IdentityFile ~/.ssh/id_rsa_scilens

#Github (default)
  Host gh
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa

#Bitbucket (secondary)
  Host bb
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/id_rsa_bb